<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.NestedScrollView
    android:id="@+id/activity_about_us"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.tuolve.lvyou.my.activity.AboutUsActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <include layout="@layout/my_title_bar"/>

        <include layout="@layout/my_pic_item"/>

        <View
            android:layout_marginTop="16dp"
            android:layout_height="3dp"
            style="@style/line_view"/>

        <include layout="@layout/contact_us_name_layout"/>
    </LinearLayout>
</android.support.v4.widget.NestedScrollView>
