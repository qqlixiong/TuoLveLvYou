<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:my="http://schemas.android.com/apk/res-auto"
              android:layout_width="match_parent"
              android:layout_height="match_parent">

    <com.tuolve.lvyou.my.customview.CustomMyView
        android:id="@+id/my_bind_phone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        my:my_name="绑定手机"
        my:my_src="@android:drawable/star_on"/>

    <com.tuolve.lvyou.my.customview.CustomMyView
        android:id="@+id/my_bind_email"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        my:my_name="绑定邮箱"
        my:my_src="@android:drawable/star_on"/>

    <com.tuolve.lvyou.my.customview.CustomMyView
        android:id="@+id/my_we_chat_account"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        my:my_name="微信账号"
        my:my_src="@android:drawable/star_on"/>

    <com.tuolve.lvyou.my.customview.CustomMyView
        android:id="@+id/my_qq_account"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        my:my_name="QQ账号"
        my:my_src="@android:drawable/star_on"/>

    <com.tuolve.lvyou.my.customview.CustomMyView
        android:id="@+id/my_tweet_account"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        my:my_name="微博账号"
        my:my_src="@android:drawable/star_on"/>

    <com.tuolve.lvyou.my.customview.CustomMyView
        android:id="@+id/my_revise_pws"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        my:my_name="修改密码"
        my:my_src="@android:drawable/star_on"/>
</merge>